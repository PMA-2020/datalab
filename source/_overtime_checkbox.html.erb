<div id='overtime-checkbox-container' class='overtime-checkbox-container form-group'>
  <h4 class='i18nable' data-key='over-time' data-toggle="tooltip" title="To chart data overtime, select multiple rounds of data from the same country.">
    Over-time
  </h4>
  <div class="onoffswitch checkbox-group">
    <input type="checkbox"
           name="onoffswitch"
           class="onoffswitch-checkbox filter overtime-check"
           id="dataset_overtime"
           disabled='disabled'>
    <label class="onoffswitch-label cc-switch-overtime" for="dataset_overtime">
      <span class="onoffswitch-inner"></span>
      <span class="onoffswitch-switch"></span>
    </label>
  </div>
</div>